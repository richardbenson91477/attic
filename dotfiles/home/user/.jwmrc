<?xml version="1.0"?>
<JWM>
    <RootMenu onroot="2">
        <Program icon="xfce4-appfinder" label="Run">xfce4-appfinder</Program>
        <Program icon="st" label="Terminal">terminal</Program>
        <Program icon="firefox" label="Web">firefox</Program>
        <Program icon="spacefm" label="Files">spacefm</Program>
        <Program icon="pavucontrol" label="Volume">pavucontrol</Program>
    </RootMenu>
    <RootMenu onroot="3">
        <Program icon="lock.png" label="Lock">slock</Program>
        <Separator/>
        <Restart label="Restart" icon="restart.png"/>
        <Exit label="Exit" confirm="true" icon="quit.png"/>
    </RootMenu>
    
    <Group>
        <Option>tiled</Option>
        <Option>aerosnap</Option>
    </Group>

    <Tray x="0" y="1050" height="32" autohide="off">
        <TrayButton icon="apps.png">root:2</TrayButton>
        <TrayButton icon="gnome-logout">root:3</TrayButton>
<!--
        <Pager labeled="true"/>
-->
        <TaskList maxwidth="216"/>
        <Dock/>
        <Clock format="%I:%M %p on %A %B %d, %Y"><Button mask="123">exec:program_of_the_day</Button></Clock>
    </Tray>

    <WindowStyle>
        <Font>Noto-12</Font>
        <Width>3</Width>
        <Height>24</Height>
        <Corner>8</Corner>
        <Foreground>#FFFFFF</Foreground>
        <Background>#5a3040</Background>
        <Outline>#000000</Outline>
        <Opacity>0.5</Opacity>
        <Active>
            <Foreground>#FFFFFF</Foreground>
            <Background>#803858</Background>
            <Outline>#000000</Outline>
            <Opacity>1.0</Opacity>
        </Active>
    </WindowStyle>
    <TrayStyle group="true" list="all">
        <Font>Noto-12</Font>
        <Background>#431323</Background>
        <Foreground>#FFFFFF</Foreground>
        <Outline>#000000</Outline>
        <Opacity>0.75</Opacity>
    </TrayStyle>
    <TaskListStyle>
      <Font>Noto-12</Font>
      <Active>
        <Foreground>#FFFFFF</Foreground>
        <Background>#803858</Background>
      </Active>
      <Foreground>#FFFFFF</Foreground>
      <Background>#5a3040</Background>
    </TaskListStyle>
    <PagerStyle>
        <Outline>#000000</Outline>
        <Foreground>#555555</Foreground>
        <Background>#333333</Background>
        <Text>#FFFFFF</Text>
        <Active>
            <Foreground>#773355</Foreground>
            <Background>#004488</Background>
        </Active>
    </PagerStyle>
    <MenuStyle>
        <Font>Noto-12</Font>
        <Width>3</Width>
        <Foreground>#FFFFFF</Foreground>
        <Background>#432333</Background>
        <Outline>#773355</Outline>
        <Active>
            <Foreground>#FFFFFF</Foreground>
            <Background>#773355</Background>
        </Active>
        <Opacity>0.85</Opacity>
    </MenuStyle>
    <PopupStyle>
        <Font>Noto-12</Font>
        <Foreground>#a0a0a0</Foreground>
        <Background>#303030</Background>
    </PopupStyle>

